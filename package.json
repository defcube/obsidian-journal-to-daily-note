    "version": "node scripts/bump-version.mjs && git add manifest.json versions.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.72.0",
    "moment": "^2.30.1",
    "obsidian": "^1.10.3",
    "prettier": "^3.7.3",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "obsidian-daily-notes-interface": "^0.9.4"
