{
  "name": "obsidian-journal-to-daily-note",
  "version": "1.0.1",
  "description": "Add entries quickly to your daily note from any file.",
  "main": "main.js",
  "scripts": {
    "build": "node esbuild.config.mjs production",
    "dev": "node esbuild.config.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --quiet",
    "lint:dead": "knip --no-progress",
